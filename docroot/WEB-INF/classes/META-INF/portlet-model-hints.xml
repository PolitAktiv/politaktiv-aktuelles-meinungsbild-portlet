<?xml version="1.0"?>

<model-hints>
	<model name="org.politaktiv.meinungsbild.infrastructure.model.Rating">
		<field name="ratingId" type="long" />
		<field name="userId" type="long" />
		<field name="subtopicId" type="long" />
		<field name="score" type="int" />
	</model>
	<model name="org.politaktiv.meinungsbild.infrastructure.model.Subtopic">
		<field name="subtopicId" type="long" />
		<field name="name" type="String" />
		<field name="url" type="String">
			<hint name="max-length">250</hint>
		</field>
		<field name="parentTopic" type="long" />
	</model>
	<model name="org.politaktiv.meinungsbild.infrastructure.model.Topic">
		<field name="topicId" type="long" />
		<field name="name" type="String" />
		<field name="communityId" type="long" />
	</model>
</model-hints>